{
  "name": "bugover/ship",
  "version": "1.0.2",
  "type": "library",
  "description": "Core package for Porto architecture.",
  "keywords": [
    "porto",
    "porto core",
    "core",
    "Porto",
    "Porto SAP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Artak Manukyan",
      "email": "manukyan.artak.21@gmail.com"
    }
  ],
  "require": {
    "php": "<=8.2.3",
    "php-64bit": "<=8.2.3",
    "ext-gettext": "*",
    "ext-json": "*",
    "composer-plugin-api": "^2.0.0",
    "bugover/laravel-repository": "^1.0.0",
    "composer/class-map-generator": "^1.0.0",
    "guzzlehttp/guzzle": "^7.0.0",
    "hashids/hashids": "^5.0",
    "laravel/framework": "^11.0.0"
  },
  "require-dev": {
    "composer/composer": "^2.0.0",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "mockery/mockery": "^1.6.12",
    "phpmd/phpmd": "^2.0.0",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nucleus\\": "src/"
    },
    "files": [
      "src/Helpers/_helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nucleus\\Providers\\NucleusServiceProvider",
        "Nucleus\\Generator\\GeneratorsServiceProvider"
      ],
      "aliases": {
        "Hashids": "Vinkla\\Hashids\\Facades\\Hashids"
      }
    },
    "class": "Nucleus\\Installer\\PluginInstaller"
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer --config=./php_cs.dist.php"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "nucleus/container-installer": true,
      "composer/package-versions-deprecated": true
    }
  }
}
